<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane minHeight="600.0" minWidth="1200.0" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #3AAFA9;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.careeranalyzer.UniversityMeritFormController">
    <children>
        <Text layoutX="396.0" layoutY="231.0" strokeType="OUTSIDE" strokeWidth="0.0" text="ENTER YOUR FSC PERCENTAGE">
            <font>
                <Font name="System Bold" size="23.0" />
            </font>
        </Text>
        <TextField fx:id="percentageField" layoutX="515.0" layoutY="261.0" prefHeight="27.0" prefWidth="171.0" promptText="e.g 86">
            <font>
                <Font size="16.0" />
            </font>
        </TextField>
        <Button fx:id="proceedButton" layoutX="563.0" layoutY="306.0" mnemonicParsing="false" onAction="#proceed" style="-fx-background-color: #17252A;" text="Proceed" textFill="#feffff">
            <font>
                <Font size="14.0" />
            </font>
            <cursor>
                <Cursor fx:constant="CLOSED_HAND" />
            </cursor>
        </Button>
        <Text fx:id="errorField" fill="#8a0101" layoutX="516.0" layoutY="249.0" strokeType="OUTSIDE" strokeWidth="0.0" />
    </children>
</AnchorPane>
